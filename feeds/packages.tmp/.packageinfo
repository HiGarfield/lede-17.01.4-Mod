		depends on PACKAGE_php7-cli || PACKAGE_php7-cgi

	config PHP7_SYSTEMTZDATA
		bool "Use system timezone data instead of php's built-in database"
		depends on PACKAGE_php7-cli || PACKAGE_php7-cgi
		select PACKAGE_zoneinfo-core
		default y
		help
